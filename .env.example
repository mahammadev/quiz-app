## Supabase (required)
# Used in lib/db.ts, lib/supabase/client.ts, and middleware.ts
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

## Supabase service role (required for server-side writes in lib/db.ts)
# lib/db.ts checks SUPABASE_SERVICE_ROLE_KEY first, then falls back to
# SUPABASE_SERVICE_KEY, SUPABASE_KEY, or NEXT_PUBLIC_SUPABASE_ANON_KEY.
SUPABASE_SERVICE_ROLE_KEY=

## Optional fallbacks (used by lib/db.ts)
SUPABASE_SERVICE_KEY=
SUPABASE_KEY=
